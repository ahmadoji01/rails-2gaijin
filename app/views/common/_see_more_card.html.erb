<div class="col-lg-3 col-md-3 col-sm-3 mx-auto" onclick="location.href='<%= search_page_path(:category => categoryname) %>';" style="width: 250px; height:100%; padding-left: 5px; padding-right: 5px;">
    <div class="card" style="box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.2), 0 3px 10px 0 rgba(0, 0, 0, 0.19);">
        <center>
            <i class="fa fa-stream fa-10x" class="card-img-top" alt="image" style="padding-top: 100px; width: 100%; height: 300px; object-fit: fill;"></i>
        </center>
        <center>
            <h4 class="card-title" style="padding-top: 10px; padding-bottom: 10px;">See More</h4>
        </center>
    </div>
</div>
